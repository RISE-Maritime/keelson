{
  "imports": {
    "@eclipse-zenoh/zenoh-ts": "npm:@eclipse-zenoh/zenoh-ts@1.6.2",
    "@bufbuild/protobuf": "npm:@bufbuild/protobuf"
  },

  "compilerOptions": {
    "lib": ["esnext", "DOM", "DOM.Iterable"],
    "types": ["deno.ns", "deno.unstable", "deno.test"],
    "moduleResolution": "bundler",
    "strict": true
  },

  "tasks": {
    "build": "deno run -A build.ts",
    "types": "deno check --emit=declaration --outDir=dist keelson/index.ts",
    "test": "deno test"
  },

  "unstable": ["sloppy-imports"]
}
